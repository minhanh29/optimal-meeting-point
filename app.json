{
  "expo": {
    "name": "OMP",
    "slug": "optimal-meeting-point",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EDF4F7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleMapsApiKey": "AIzaSyAXtp-vw6IoEEWX6aVYD-Ug-2Qkp6uT-jE"
      },
      "bundleIdentifier": "com.omprmit.mobile"
    },
    "android": {
      "package": "com.omprmit.mobile",
	  "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/my-adaptive-icon.png",
        "backgroundColor": "#EDF4F7"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
		  "googleMaps": {
			  "apiKey": "AIzaSyAXtp-vw6IoEEWX6aVYD-Ug-2Qkp6uT-jE"
		  }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e4e99ec6-4162-4265-9c7f-7fcf09baab7d"
      }
    }
  }
}
